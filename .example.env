# API Keys and Configuration
# Copy this file to .env.local and fill in your actual API keys

# Google AI API Key for Genkit
GEMINI_API_KEY=your_gemini_api_key_here

# Resta.vn API Authentication Token
RESTA_AUTH_TOKEN=your_resta_auth_token_here
NEXT_PUBLIC_RESTA_AUTH_TOKEN=your_resta_auth_token_here

# Geoapify API Key for maps and geocoding
GEOAPIFY_API_KEY=your_geoapify_api_key_here
NEXT_PUBLIC_GEOAPIFY_API_KEY=your_geoapify_api_key_here

# AI Search API Keys
# Perplexity AI API Key for real estate search
PERPLEXITY_API_KEY=your_perplexity_api_key_here
PERPLEXITY_API_URL=https://api.perplexity.ai/chat/completions
PERPLEXITY_MODEL=sonar-pro

# Proxy Server Configuration (Primary AI provider)
PROXY_SERVER_URL=your_proxy_server_url_here
PROXY_SERVER_API_KEY=your_proxy_server_api_key_here
PROXY_SERVER_ENABLED=true

# Jina AI API Key (deprecated, for legacy support)
JINA_API_KEY=your_jina_api_key_here

# Guland FastAPI Server URL
# Format: http://your-server-ip:port or https://your-domain.com
NEXT_PUBLIC_GULAND_SERVER_URL=your_server_url_here
NEXT_PUBLIC_GULAND_BEARER=your_api_key

# AI Server Proxy Configuration (Alternative naming)
AI_SERVER_PROXY_URL=your_ai_server_proxy_url_here
AI_SERVER_PROXY_API_KEY=your_ai_server_proxy_api_key_here

#MAPBOX 
NEXT_PUBLIC_MAPBOX_ACCESS_TOKEN=your_mapbox_api_key_here
